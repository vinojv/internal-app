<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="SNOW_CMDB_USER" TYPE="[USER]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
	<fileparameters>
		<parameter name="headerRowCount"  value="1" />
		<parameter name="dateFormatMask"  value="MM-DD-YYYY HH24:MI:SS" />
		<parameter name="fieldDelimiter" type="ascii" value="44" />
		<parameter name="recordDelimiter" type="namedDelimiter" value="NEWLINE" />
	</fileparameters>
	<COLUMN NAME="TENANT_DATA_PARTITION_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="IF_SOURCE_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="TENANT_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="DATA_PARTITION_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="SOURCE_CODE" TYPE="virtual" VALUE="SNOW" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="DATE_FORMAT_MASK" TYPE="virtual" VALUE="MM-dd-yyyy HH:mm:ss" PROCESS="TRIM"  />
	<COLUMN NAME="NATIVE_USER_ID" EXTERNALNAME="u_guid" TYPE="identifier" PROCESS="TRIM"  />
	<COLUMN NAME="name" EXTERNALNAME="name" TYPE="ignore"  />
	<COLUMN NAME="user_name" EXTERNALNAME="user_name" TYPE="ignore"  />
	<COLUMN NAME="EMPLOYEE_CODE" EXTERNALNAME="employee_number" TYPE="attribute" PROCESS="TRIM"  />
	<COLUMN NAME="email" EXTERNALNAME="email" TYPE="ignore"  />
	<COLUMN NAME="phone" EXTERNALNAME="phone" TYPE="ignore"  />
	<COLUMN NAME="mobile_phone" EXTERNALNAME="mobile_phone" TYPE="ignore"  />
	<COLUMN NAME="home_phone" EXTERNALNAME="home_phone" TYPE="ignore"  />
	<COLUMN NAME="title" EXTERNALNAME="title" TYPE="ignore"  />
	<COLUMN NAME="introduction" EXTERNALNAME="introduction" TYPE="ignore"  />
	<COLUMN NAME="first_name" EXTERNALNAME="first_name" TYPE="ignore"  />
	<COLUMN NAME="middle_name" EXTERNALNAME="middle_name" TYPE="ignore"  />
	<COLUMN NAME="last_name" EXTERNALNAME="last_name" TYPE="ignore"  />
	<COLUMN NAME="USER_STATUS" EXTERNALNAME="active" TYPE="attribute" PROCESS="TRIM"  />
	<COLUMN NAME="location_u_guid" EXTERNALNAME="location.u_guid" TYPE="ignore"  />
	<COLUMN NAME="location_country" EXTERNALNAME="location.country" TYPE="ignore"  />
	<!-- <COLUMN NAME="IGNORE_RECORD" TYPE="virtual" VALUE="=if(rec.get('USER_STATUS') == 'true') return false; else return true;"  /> -->
	<COLUMN NAME="FILTER" TYPE="VIRTUAL" VALUE="= var isActive = upper(rec.get('USER_STATUS')); if(isActive != null) if(isActive == 'TRUE') return 'true'; return null;" CONDITION="NOT NULL" />
	<COLUMN NAME="TIME_ZONE" TYPE="virtual" VALUE="US/Michigan"  />
</blz:FEED>

