<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="SNOW_REF_SUPPORT_GROUP" TYPE="[REFERENCE]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
