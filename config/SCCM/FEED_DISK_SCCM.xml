<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="SCCM_DISK" TYPE="[DISK]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
	<fileparameters>
		<parameter name="headerRowCount"  value="1" />
		<parameter name="dateFormatMask"  value="yyyy-MM-dd HH:mm:ss.SSS" />
		<parameter name="fieldDelimiter" type="ascii" value="124" />
		<parameter name="recordDelimiter" type="namedDelimiter" value="NEWLINE" />
	</fileparameters>
	<COLUMN NAME="TENANT_DATA_PARTITION_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="IF_SOURCE_ID" TYPE="virtual" VALUE="3" />
    <COLUMN NAME="TENANT_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="DATA_PARTITION_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="SOURCE_CODE" TYPE="virtual" VALUE="SCCM" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="DATE_FORMAT_MASK" TYPE="virtual" VALUE="yyyy-MM-dd HH:mm:ss.SSS" PROCESS="TRIM" />
    <COLUMN NAME="DISK_SERIAL_NUMBER" TYPE="identifier" EXTERNALNAME="DiskSerialNumber" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="SCAN_DATE" TYPE="attribute" EXTERNALNAME="Timestamp" RECENCY_INDICATOR="LATEST" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)" />
    <COLUMN NAME="SCAN_DATE_O" TYPE="attribute" EXTERNALNAME="Timestamp" />	
	<COLUMN NAME="DEVICE_ID" TYPE="attribute" EXTERNALNAME="DeviceID" PROCESS="TRIM" />
	<COLUMN NAME="INTERFACE_TYPE" TYPE="attribute" EXTERNALNAME="InterfaceType" PROCESS="TRIM" />	
	<COLUMN NAME="DISK_MODEL" TYPE="attribute" EXTERNALNAME="DiskModel" PROCESS="TRIM" />
	<COLUMN NAME="NUMBER_OF_PARTITIONS" TYPE="attribute" EXTERNALNAME="NumberPartitions" PROCESS="EXTRACT_FIRST_NUMERIC" />
	<COLUMN NAME="DISK_SIZE" TYPE="attribute" EXTERNALNAME="Size" />
	<COLUMN NAME="DISK_SIZE_UNITS" TYPE="virtual" VALUE="MB" />
	<COLUMN NAME="ASSET_NAME" TYPE="identifier" EXTERNALNAME="SystemName" />
	<COLUMN NAME="DRIVE_TYPE" TYPE="attribute" EXTERNALNAME="MediaType"  />
	<COLUMN NAME="DISK_NAME" TYPE="virtual" VALUE="= var an = rec.get('ASSET_NAME'); var dsn = rec.get('DISK_SERIAL_NUMBER'); var result = ''; if(an != null) result+=an+'-'; else result+='-'; if(dsn != null) result+=dsn; return result;"  />
	<COLUMN NAME="TIME_ZONE" TYPE="virtual" VALUE="US/Michigan"  />
</blz:FEED>