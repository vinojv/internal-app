<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="SCCM_NET_ADAPTER" TYPE="[NETWORK_INTERFACE]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
	<fileparameters>
		<parameter name="headerRowCount"  value="1" />
		<parameter name="dateFormatMask"  value="yyyy-MM-dd HH:mm:ss.SSS" />
		<parameter name="fieldDelimiter" type="ascii" value="124" />
		<parameter name="recordDelimiter" type="namedDelimiter" value="NEWLINE" />
	</fileparameters>
	<COLUMN NAME="TENANT_DATA_PARTITION_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="IF_SOURCE_ID" TYPE="virtual" VALUE="3" />
    <COLUMN NAME="TENANT_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="DATA_PARTITION_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="SOURCE_CODE" TYPE="virtual" VALUE="SCCM" PROCESS="UPPER_TRIM" />
    <COLUMN NAME="DATE_FORMAT_MASK" TYPE="virtual" VALUE="yyyy-MM-dd HH:mm:ss.SSS" PROCESS="TRIM" />
    <COLUMN NAME="ASSET_NAME" TYPE="identifier" EXTERNALNAME="SystemName" />
	<COLUMN NAME="MAC_ADDRESS" TYPE="identifier" EXTERNALNAME="MACAddress" PROCESS="UPPER_TRIM" />
	<COLUMN NAME="SCAN_DATE" TYPE="attribute" EXTERNALNAME="Timestamp" RECENCY_INDICATOR="LATEST" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)" />
	<COLUMN NAME="SCAN_DATE_O" TYPE="attribute" EXTERNALNAME="Timestamp" />
	<COLUMN NAME="NET_ADAPTER_TYPE" TYPE="attribute" EXTERNALNAME="AdapterType" />
	<COLUMN NAME="DEVICE_ID" TYPE="attribute" EXTERNALNAME="DeviceID" />
	<COLUMN NAME="ADAPTER_MANUFACTURER" TYPE="attribute" EXTERNALNAME="Manufacturer" PROCESS="TRIM"  />	
	<COLUMN NAME="ADAPTER_MODEL" TYPE="identifier" EXTERNALNAME="ProductName" PROCESS="TRIM" />
	<COLUMN NAME="SERVICE_NAME" TYPE="ignore" EXTERNALNAME="ServiceName" />
	<COLUMN NAME="NET_ADAPTER_NAME" TYPE="virtual" VALUE="= var an = rec.get('ASSET_NAME'); var am = rec.get('ADAPTER_MODEL'); var result = ''; if(an != null) result+=an+'-'; else result+='-'; if(am != null) result+=am; return result;" />	
	<COLUMN NAME="TIME_ZONE" TYPE="virtual" VALUE="US/Michigan"  />
</blz:FEED>