<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="TIMS_EUC_SERVER" TYPE="[ASSET]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
	<fileparameters>
		<parameter name="headerRowCount"  value="2" />
		<parameter name="dateFormatMask"  value="MM/DD/YYYY HH:MI:SS PM" />
		<parameter name="fieldDelimiter" type="ascii" value="124" />
		<parameter name="recordDelimiter" type="namedDelimiter" value="NEWLINE" />
	</fileparameters>
	<COLUMN NAME="TENANT_DATA_PARTITION_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="IF_SOURCE_ID" TYPE="virtual" VALUE="2" />
