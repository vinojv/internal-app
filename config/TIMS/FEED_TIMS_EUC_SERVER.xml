	<COLUMN NAME="ASSET_INSTALL_DATE_O" EXTERNALNAME="Server Install Date" TYPE="attribute" />
	<COLUMN NAME="DECOMMISSION_DATE" EXTERNALNAME="Server Uninstall Date" TYPE="attribute" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)"/>
	<COLUMN NAME="DECOMMISSION_DATE_O" EXTERNALNAME="Server Uninstall Date" TYPE="attribute" />
	<COLUMN NAME="MAINTENANCE_LOCATION_CODE" EXTERNALNAME="Maintenance Location Code" TYPE="ignore" />
	<COLUMN NAME="SERVER_TIER" EXTERNALNAME="Server Tier" TYPE="ignore" />
