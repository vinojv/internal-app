	<COLUMN NAME="LASTLOGON" EXTERNALNAME="lastLogon" TYPE="ignore"  />
	<COLUMN NAME="SCAN_DATE_O" EXTERNALNAME="lastLogonTimestamp" TYPE="attribute"  />
	<COLUMN NAME="SCAN_DATE" EXTERNALNAME="lastLogonTimestamp" TYPE="attribute" RECENCY_INDICATOR="LATEST" PROCESS="CONVERT_STRING_TO_DATE('LDAP',TIME_ZONE)"  />
	<COLUMN NAME="LOCKOUTTIME" EXTERNALNAME="lockoutTime" TYPE="ignore"  />
	<COLUMN NAME="LOGONCOUNT" EXTERNALNAME="logonCount" TYPE="ignore"  />
	<COLUMN NAME="MANAGEDBY" EXTERNALNAME="managedBy" TYPE="ignore"  />
