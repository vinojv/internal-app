<?xml version="1.0" encoding="utf-8" ?>
<blz:FEED NAME="AD" TYPE="[ASSET]" ISPRIMARY="TRUE" PROPAGATE="FALSE" xmlns:blz="http://www.blazent.com/xmlbeans/connectorconfig" >
	<fileparameters>
		<parameter name="headerRowCount"  value="1" />
		<parameter name="dateFormatMask"  value="MM/DD/YYYY" />
		<parameter name="fieldDelimiter" type="ascii" value="124" />
		<parameter name="recordDelimiter" type="namedDelimiter" value="NEWLINE" />
	</fileparameters>
	<COLUMN NAME="TENANT_DATA_PARTITION_ID" TYPE="virtual" VALUE="1" />
	<COLUMN NAME="IF_SOURCE_ID" TYPE="virtual" VALUE="17" />
	<COLUMN NAME="TENANT_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="DATA_PARTITION_CODE" TYPE="virtual" VALUE="AMEX" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="SOURCE_CODE" TYPE="virtual" VALUE="AD" PROCESS="UPPER_TRIM"  />
	<COLUMN NAME="DATE_FORMAT_MASK" TYPE="virtual" VALUE="yyyyMMdd" PROCESS="TRIM"  />
	<COLUMN NAME="OBJECTGUID" EXTERNALNAME="objectGUID" TYPE="ignore"  />
	<COLUMN NAME="ACCOUNTEXPIRES" EXTERNALNAME="accountExpires" TYPE="ignore"  />
	<COLUMN NAME="ADMINCOUNT" EXTERNALNAME="adminCount" TYPE="ignore"  />
	<COLUMN NAME="BADPASSWORDTIME" EXTERNALNAME="badPasswordTime" TYPE="ignore"  />
	<COLUMN NAME="BADPWDCOUNT" EXTERNALNAME="badPwdCount" TYPE="ignore"  />
	<COLUMN NAME="canonicalName" EXTERNALNAME="canonicalName" TYPE="ignore"  />
	<COLUMN NAME="ASSET_COMMENT" EXTERNALNAME="comment" TYPE="attribute" PROCESS="TRIM"  />
	<COLUMN NAME="CREATETIMESTAMP" EXTERNALNAME="createTimeStamp" TYPE="ignore"  />
	<COLUMN NAME="ASSET_DESCRIPTION" EXTERNALNAME="description" TYPE="attribute" PROCESS="TRIM"  />
