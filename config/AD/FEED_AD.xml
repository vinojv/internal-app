	<COLUMN NAME="MONITORINGAVAILABILITYWINDOW" EXTERNALNAME="monitoringAvailabilityWindow" TYPE="ignore"  />
	<COLUMN NAME="ASSET_NAME" EXTERNALNAME="name" TYPE="identifier"   />
	<COLUMN NAME="OS_NAME" EXTERNALNAME="operatingSystem" TYPE="attribute"   />
	<COLUMN NAME="OPERATINGSYSTEMHOTFIX" EXTERNALNAME="operatingSystemHotfix" TYPE="ignore"  />
	<COLUMN NAME="OS_SERVICE_PACK" EXTERNALNAME="operatingSystemServicePack" TYPE="attribute"   />
	<COLUMN NAME="OS_VERSION" EXTERNALNAME="operatingSystemVersion" TYPE="attribute"   />
	<COLUMN NAME="PWDLASTSET" EXTERNALNAME="pwdLastSet" TYPE="ignore"  />
	<COLUMN NAME="SERVICEPRINCIPALNAME" EXTERNALNAME="servicePrincipalName" TYPE="ignore"  />
