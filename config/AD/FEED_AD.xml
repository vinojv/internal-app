	<COLUMN NAME="MONITOREDCONFIGURATIONS" EXTERNALNAME="monitoredConfigurations" TYPE="ignore"  />
	<COLUMN NAME="MONITOREDSERVICES" EXTERNALNAME="monitoredServices" TYPE="ignore"  />
