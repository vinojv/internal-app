	<COLUMN NAME="WHENCREATED" EXTERNALNAME="whenCreated" TYPE="ignore"  />
	<COLUMN NAME="RECORD_UPDATED_DATE" TYPE="virtual" VALUE="=return trim(substr(rec.get('WHENCHANGED'),1,(length(rec.get('WHENCHANGED'))-9)));" DATATYPE="noquote" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)"  />
	<COLUMN NAME="RECORD_CREATED_DATE" TYPE="virtual" VALUE="=return trim(substr(rec.get('WHENCREATED'),1,(length(rec.get('WHENCREATED'))-9)));" DATATYPE="noquote" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)"  />
	<COLUMN NAME="ASSET_INSTALL_DATE" TYPE="virtual" VALUE="=return trim(substr(rec.get('CREATETIMESTAMP'),1,(length(rec.get('CREATETIMESTAMP'))-9)));" DATATYPE="noquote" PROCESS="CONVERT_STRING_TO_DATE(DATE_FORMAT_MASK,TIME_ZONE)"  />
	<COLUMN NAME="TIME_ZONE" TYPE="virtual" VALUE="US/Michigan"  />
</blz:FEED>

